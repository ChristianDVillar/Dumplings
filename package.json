{
  "name": "dumplings-menu-app",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:web": "npx expo export:web",
    "build:web:verbose": "npx expo export:web --verbose",
    "preview": "expo export:web && (test -d web-build && npx serve web-build || npx serve dist)",
    "server": "node server/index.js",
    "db:seed": "node database/seed.js",
    "db:seed:users": "node database/seedUsers.js",
    "db:init": "node -e \"require('./database/dbService').initializeTables().then(() => process.exit(0)).catch(e => { console.error(e); process.exit(1); })\"",
    "db:test": "node -e \"require('./database/dbService').query('SELECT 1').then(() => { console.log('✓ Conexión exitosa'); process.exit(0); }).catch(e => { console.error('✗ Error:', e.message); process.exit(1); })\""
  },
  "dependencies": {
    "@expo/webpack-config": "^19.0.0",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "expo": "~49.0.0",
    "expo-status-bar": "~1.6.0",
    "expo-updates": "~0.18.19",
    "express": "^5.2.1",
    "html5-qrcode": "^2.3.8",
    "pg": "^8.17.2",
    "qrcode.react": "^4.2.0",
    "react": "18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "0.72.10",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-web": "^0.19.13"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0"
  },
  "private": true
}
